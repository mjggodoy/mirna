<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="mirna.beans.MiRna" table="mirna">
        <id name="pk" type="int">
            <column name="pk" precision="5" scale="0" />
            <generator class="assigned" />
        </id>
        <property name="name" type="string">
            <column name="name" length="20" not-null="true" />
        </property>
        <property name="accessionNumber" type="string">
            <column name="accession_number" length="20" not-null="true" />
        </property>
        <property name="sequence" type="string">
            <column name="sequence" length="20" not-null="true" />
        </property>
        <property name="resource" type="string">
            <column name="resource" length="20" not-null="true" />
        </property>
    </class>
</hibernate-mapping>